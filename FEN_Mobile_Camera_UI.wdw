 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1+
      PROCEDURE MaFenêtre(LOCAL bAutoriseVidéo = vrai)
      
      // Initialise le plan initial à afficher
      CAM_Capture..Caméra=camCaméraDorsale
      CAM_Capture..Zoom=1
      CAM_Capture..Flash=camFlashAuto
      CAM_Capture..Torche=camTorcheOff
      mafenetre..Plan=PLAN_Capture_Photo
      
      // Affiche l'image de bascule vers le mode vidéo, ou masque l'image si l'enregistrement de vidéo est interdit
      IMG_ModeVidéo..Visible = bAutoriseVidéo
      
      // Identifiant du timer qui appelle périodiquement SurTempsEcoulé
      gnTimerVidéoTempsEcoulé est un entier = -1
      
      // Chemin de l'image/video qui sera générée
      gsChemin est une chaîne
   -
     code : |1+
      // Initialise l'UI au 1er affichage
      // Note : les différents éléments de l'UI ont automatiquement ajouté des procédures internes notifiées lors de cet appel à DemandeMiseAJourUI()
      DemandeMiseAJourUI()
     type : 34
   -
     code : |1+
      // Calcul du temps écoulé dans un enregistrement vidéo
      dhDébutEnregistrement est une DateHeure
      
      // Formate le temps écoulé si on est en cours d'enregistrement d'une vidéo
      si MaFenetre..Plan = PLAN_Enregistrement_en_cours ALORS
      	si gnTimerVidéoTempsEcoulé = -1 ALORS		
      	 	// Formate la durée écoulée à chaque appel du timer
      	 	gnTimerVidéoTempsEcoulé = TimerSys(SurTempsEcoulé,250ms)
      	 	procédure interne SurTempsEcoulé
      	 		dhMaintenant est une dateheure
      	 		duréeEcoulée est une durée = dhMaintenant - dhDébutEnregistrement		
      	 		LIB_TempsEcoulé = DuréeVersChaîne(duréeEcoulée,"HH:MM:SS")
      	 	FIN
      	 	// 1er appel pour initialiser le temps écoulé
      	 	SurTempsEcoulé()
      	FIN	
      // Arrête le calcul du temps écoulé
      sinon si gnTimerVidéoTempsEcoulé>-1
      	FinTimerSys(gnTimerVidéoTempsEcoulé)
      	gnTimerVidéoTempsEcoulé = -1
      FIN
      
      // Si la torche est allumée et que l'on quitte l'enregistrement vidéo, on doit éteindre la torche
      Si CAM_Capture..Torche=camTorcheOn _et_ MaFenêtre..Plan<>PLAN_Capture_Vidéo _et_ MaFenêtre..Plan<>PLAN_Enregistrement_en_cours  alors
      	CAM_Capture..Torche=camTorcheOff
      FIN
      
     type : 234
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      // Éteint la torche en fermeture de fenêtre
      CAM_Capture..Torche=camTorcheOff
      
      // Renvoie à la fenêtre appelante le chemin de l'image ou de la vidéo capturée 
      renvoyer gsChemin
     type : 2
  procedures :
   -
     name : PrendrePhoto
     procedure_id : 1207932241751891390
     type_code : 14
     code : |1-
      // Prend une photo à partir du champ caméra
      procédure PrendrePhoto()
      
      cameraphoto(CAM_Capture, SurCapturePhoto)
      procédure interne SurCapturePhoto(bOK, sCheminPhoto)
      	si pas bOK ALORS
      		ToastAffiche(<§Rff00009b8fac5d7f¤fr-FR¤Erreur rencontrée lors de la prise de photo.¤en-GB¤¤es-ES¤§> + [RC] + Erreurinfo())
      		renvoyer faux
      	FIN
      	// Mémorise le chemin de la photo prise
      	gsChemin = sCheminPhoto
      	IMG_Aperçu=sCheminPhoto
      	MaFenetre..Plan = PLAN_Aperçu_de_photo
      	// Met à jour l'affichage
      	DemandeMiseAJourUI()
      	
      	renvoyer vrai
      FIN
      
      // Note: ici il est possible de personnaliser l'affichage pendant la capture de photo en cours
     type : 458752
   -
     name : DémarrerEnregistrementVidéo
     procedure_id : 1207937752195167222
     type_code : 14
     code : |1-
      procédure DémarrerEnregistrementVidéo()
      
      // Démarre l'enregistrement d'une vidéo à partir du champ caméra
      CaméraVidéoDémarre(CAM_Capture,SurFinEnregistrementVidéo)
      
      PROCÉDURE INTERNE SurFinEnregistrementVidéo(bOK, sCheminVidéo)
      	SI PAS bOK ALORS
      		ToastAffiche(<§Rff00019b8fae5d7f¤fr-FR¤Erreur rencontrée lors de l'enregistrement de la vidéo.¤en-GB¤¤es-ES¤§> + [RC] + Erreurinfo())
      		RENVOYER Faux
      	FIN
      	// Mémorise le chemin de la vidéo prise
      	gsChemin	= sCheminVidéo
      	MM_Aperçu=gsChemin
      	MaFenetre..Plan	= PLAN_Aperçu_de_vidéo
      	// Met à jour l'affichage
      	DemandeMiseAJourUI()
      	RENVOYER Vrai
      FIN
      
      // Affiche l'enregistrement en cours
      MaFenetre..Plan = PLAN_Enregistrement_en_cours
      DemandeMiseAJourUI()
     type : 458752
   -
     name : ModifieZoom
     procedure_id : 1220839888340823763
     type_code : 14
     code : |1+
      procédure ModifieZoom(rValeurZoomBouton)
      
      // Mise à jour de l'UI lors d'un zoom
      si CAM_Capture..Zoom<>rValeurZoomBouton ALORS
      	CAM_Capture..Zoom=rValeurZoomBouton	
      FIN
      
      LIB_ZOOM.FormateLibelléZoom(CAM_Capture..Zoom)
     type : 458752
   -
     name : RetourHaptique
     procedure_id : 1238309045420821116
     type_code : 14
     code : |1-
      procédure RetourHaptique()
      
      // Permet d'indiquer à l'utilisateur la détection d'action par une légère vibration
      <COMPILE SI TypeConfiguration=Android>
      	VibrationDéclenche(10ms)
      <FIN> 
     type : 458752
