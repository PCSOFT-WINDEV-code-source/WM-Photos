        type : 14
     procedures :
      -
        name : FormateLibelléZoom
        internal_properties : CAAAAAgAAADaa/V/OxRssS4Cz7jPSUvuaDHW1cr3mTw1G1qUkAGQd7bD8fE3u/GkabZreVhw1insOwVgOmStDFuLGOKLKhWIDesPuNwy2+7r5CqVx1qjKM9MByrBFOO4+hQBol8yswKnJJ4lhkvq05SW/uUB
        procedure_id : 1220837599123115882
        type_code : 14
        code : |1+
         // Applique le zoom sur le champ Caméra et fait apparaitre le libellé indiquant le zoom si la valeur de zoom est différente de la valeur initiale
         procédure FormateLibelléZoom(rZoom, LOCAL bRendVisibleSiNecessaire=vrai)
         // Vérifie si la valeur a changé
         soit sNouveauLibelleZoom = ChaîneConstruit(<§R11eab9cc1614bbac¤fr-FR¤%1x¤en-GB¤¤es-ES¤§>, Arrondi(rZoom,1))
         // Si la valeur a changé, il faut afficher le zoom
         si bRendVisibleSiNecessaire _et_ sNouveauLibelleZoom=MonChamp..Libellé ALORS
         	bRendVisibleSiNecessaire=Faux
         FIN
         // Met à jour le libellé
         MonChamp..Libellé=sNouveauLibelleZoom
         // si le libellé était invisible et que l'on doit le rendre visible, on le fait immediatement
         si bRendVisibleSiNecessaire alors
         	MonChamp..VisibleAvecAnimation=vrai
         	si gnTimerInvisible<>0 alors 
         		FinTimerSys(gnTimerInvisible)
         	FIN
         	gnTimerInvisible=TimerSys(MasqueAprèsDélai, 1s)
         FIN
         
         // Masque le champ après un certain délai si aucun autre zoom a été effectué
         procédure interne MasqueAprèsDélai()
         	FinTimerSys(gnTimerInvisible)
         	gnTimerInvisible=0
         	MonChamp..VisibleAvecAnimation=Faux
         fin
        type : 458752
