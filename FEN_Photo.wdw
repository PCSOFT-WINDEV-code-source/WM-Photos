     procedure_id : 1755243058178476492
     type_code : 14
     code : |1-
      // Résumé : Supprime la photo
      //
      PROCEDURE SupprimerPhoto()
      
      HLitRecherchepremier(Photo,IDPhoto,gnIDPhoto)
      si HTrouve(Photo) ALORS
      	// Suppression des fichiers physiques
      	fSupprime(PhotoChemin(Photo.Chemin))
      	fSupprime(PhotoCheminMiniature(Photo.Chemin))
      	
      	// Suppression dans la base
      	HSupprime(Photo)
      	
      FIN
      	
      MaFenêtre..ValeurRenvoyée = "suppr"	
      ferme
